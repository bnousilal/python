str = "Hello"
print(f"address of the string: {id(str)}")
str += "!"
print(f"address of the string: {id(str)}")

list1=["nousi", "riya", "rishik", "alekhya"]
list2=list1
print(f"address of the list1 {id(list1)}, {list1}");
print(f"address of the list2 {id(list2)}, {list2}");
list2.append("dinesh")
print(f"address of the list1 {id(list1)}, {list1}");
print(f"address of the list2 {id(list2)}, {list2}");
